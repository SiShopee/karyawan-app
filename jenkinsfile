pipeline {
        agent any
        stages {
            stage('Checkout') {
                steps {
                    git url: 'https://github.com/SiShopee/karyawan-app.git', branch: 'main'
                 }
        }

	stage('Install Dependencies') {
	    steps {
		sh 'python3 -m venv venv'
		sh '. venv/bin/activate && pip install --upgrade pip'
	    }
	}

